{
  "name": "zustand-forms",
  "version": "0.4.1",
  "description": "**fast typesafe form states as zustand stores**",
  "main": "./lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Conduct/zustand-forms.git"
  },
  "keywords": [
    "form",
    "forms",
    "zustand",
    "react",
    "typescript",
    "validation"
  ],
  "author": "Hugo McPhee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Conduct/zustand-forms/issues"
  },
  "homepage": "https://github.com/Conduct/zustand-forms#readme",
  "typings": "./lib/index.d.ts",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "babel-eslint": "^10.1.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7",
    "react": "^16.13.1",
    "@types/react": "^16.9.43"
  },
  "files": [
    "lib/**/*"
  ],
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "dependencies": {
    "immer": "^7.0.7",
    "zustand": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  }
}
